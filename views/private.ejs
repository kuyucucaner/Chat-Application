<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="https://cdn.socket.io/4.7.2/socket.io.min.js"></script>
  <link rel="stylesheet" href="./stylesheets/private.css">
  <title>Private Chat</title>
</head>
<body>
    <div id="messages-container">
        <ul id="messages"></ul>
      </div>
  <form id="private-form">
    <label for="to-username">To:</label>
    <input type="text" id="to-username" required />
    <input id="private-m" autocomplete="off" />
    <button type="button" onclick="sendPrivateMessage()">Send Private Message</button>
  </form>
  <br>
  <button type="button" onclick="goToProfile()">Profile</button>
  <br>
  <button type="button" onclick="goToChat()">Chat</button>
  <br>
  <script src="http://localhost:3000/socket.io/socket.io.js"></script>
  <script src='./javascripts/goToProfile.js'></script>
  <script src='./javascripts/goToChat.js'></script>
  <script src='./javascripts/private.js'></script>
</body>
</html>
